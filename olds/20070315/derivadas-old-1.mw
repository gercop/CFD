<Worksheet><Version major="6" minor="1"/><View-Properties><Zoom percentage="100"/></View-Properties><Styles><Layout alignment="left" firstindent="0.0" leftmargin="0.0" linespacing="0.0" name="Warning" rightmargin="0.0" spaceabove="0.0" spacebelow="0.0"/><Layout alignment="left" firstindent="0.0" leftmargin="0.0" linespacing="0.0" name="Normal" rightmargin="0.0" spaceabove="0.0" spacebelow="0.0"/><Layout alignment="centred" firstindent="0.0" leftmargin="0.0" linespacing="0.5" name="Maple Output" rightmargin="0.0" spaceabove="0.0" spacebelow="0.0"/><Layout leftmargin="0.0" name="_pstyle5" rightmargin="0.0"/><Layout alignment="left" firstindent="0.0" leftmargin="0.0" linebreak="space" linespacing="0.0" name="_pstyle4" rightmargin="0.0" spaceabove="8.0" spacebelow="2.0"/><Layout alignment="left" firstindent="0.0" leftmargin="0.0" linebreak="space" linespacing="0.0" name="_pstyle3" rightmargin="0.0" spaceabove="0.0" spacebelow="0.0"/><Layout alignment="left" firstindent="0.0" leftmargin="0.0" linebreak="space" linespacing="0.0" name="_pstyle2" rightmargin="0.0" spaceabove="0.0" spacebelow="0.0"/><Layout alignment="left" firstindent="0.0" leftmargin="0.0" linebreak="space" linespacing="0.0" name="_pstyle1" rightmargin="0.0" spaceabove="8.0" spacebelow="4.0"/><Font background="[0,0,0]" bold="true" executable="true" family="Monospaced" foreground="[255,0,0]" name="Maple Input" readonly="false"/><Font background="[0,0,0]" bold="false" executable="false" family="Monospaced" foreground="[0,0,255]" italic="false" name="Warning" readonly="true" size="10" underline="false"/><Font background="[0,0,0]" executable="false" name="_pstyle5" readonly="false"/><Font background="[0,0,0]" bold="true" executable="false" family="Times New Roman" foreground="[0,0,0]" italic="false" name="_cstyle4" readonly="false" size="14" underline="false"/><Font background="[0,0,0]" bold="false" executable="false" family="Times New Roman" foreground="[0,0,0]" italic="false" name="_cstyle3" readonly="false" size="12" underline="false"/><Font background="[0,0,0]" bold="true" executable="true" family="Monospaced" foreground="[255,0,0]" italic="false" name="_cstyle2" readonly="false" size="12" underline="false"/><Font background="[0,0,0]" bold="true" executable="false" family="Times New Roman" foreground="[0,0,0]" italic="false" name="_cstyle1" readonly="false" size="18" underline="false"/><Font background="[0,0,0]" bold="false" executable="false" family="Times New Roman" foreground="[0,0,0]" italic="false" name="ParagraphStyle1" readonly="false" size="12" underline="false"/><Font background="[0,0,0]" executable="false" family="Times New Roman" foreground="[0,0,255]" name="2D Output" readonly="false" underline="false"/><Font background="[0,0,0]" executable="false" family="Times New Roman" name="Page Number" readonly="false" underline="false"/></Styles><Page-Numbers enabled="false" first-number="1" first-numbered-page="1" horizontal-location="right" style="Page Number" vertical-location="bottom"/><Section collapsed="true"><Title><Text-field layout="_pstyle1" style="_cstyle1"><Font encoding="ISO8859-1">Calculo da viscosidade din\342mica</Font></Text-field></Title><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">restart;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">mu := T^(3/2)*(1+C2)/(T+C2);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">diff(mu,T);</Text-field></Input></Group></Section><Section collapsed="true"><Title><Text-field layout="_pstyle1" style="_cstyle1">Base profile and disturbances - 2D</Text-field></Title><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">restart;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">u_0(y) := 1/2*(U1+U2)+(U1-U2)/2*tanh(sigma*y);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">diff(u_0(y),y);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">diff(diff(u_0(y),y),y);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">v_1 := A*cos(alpha*x-omega*t)*exp(-sigma*y^2); </Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">u_1 := int(diff(v_1,y),x);</Text-field></Input></Group></Section><Section collapsed="true"><Title><Text-field layout="_pstyle1" style="_cstyle1">Base profile and Disturbances - 3D</Text-field></Title><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">restart;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">u_0(y) := 1/2*(U1+U2)+(U1-U2)/2*tanh(sigma*y);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">diff(u_0(y),y);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">diff(diff(u_0(y),y),y);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">w_1 := (A_1*cos(alpha*x-omega*t+phi)+A_2*cos(alpha*x+beta*z-omega*t)+A_2*cos(alpha*x-beta*z-omega*t))*exp(-sigma*y^2); </Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">v_1 := (A_1*cos(alpha*x-omega*t+phi)+A_2*cos(alpha*x+beta*z-omega*t)+A_2*cos(alpha*x-beta*z-omega*t))*exp(-sigma*y^2); ;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">u_1 := -int(diff(w_1,z),x)-int(diff(v_1,y),x);</Text-field></Input></Group><Text-field layout="_pstyle1" style="_cstyle1"/></Section><Section collapsed="true"><Title><Text-field layout="_pstyle1" style="_cstyle1">Variables transformation - Stretching for Shear Layer</Text-field></Title><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">restart;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">y:=B+1/sp*arcsinh((y_f/y_c-1.0)*sinh(sp*B));</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">diff(y,y_f);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">diff(diff(y,y_f),y_f);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fy := cy_c*(1.0+sinh(sp*(cy-B))/sinh(sp*B) );</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">diff(fy,cy);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">diff(diff(fy,cy),cy);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle3" style="_cstyle3"/></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">restart;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle3" style="_cstyle3"/></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">y_b := 1 - log((B+1-y/h)/(B-1+y/h))/log((B+1)/(B-1)) ;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">dy_b := simplify(diff(y_b,y));</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">d2y_b := simplify(diff(diff(y_b,y),y));</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">with(codegen,fortran):</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fortran(y_b);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fortran(dy_b);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fortran(d2y_b);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2"/></Input></Group></Section><Section collapsed="true"><Title><Text-field layout="_pstyle1" style="_cstyle1">Esquema de quinta ordem explicito:</Text-field></Title><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">restart;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle3" style="_cstyle3"/></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">Lx_1 :=(x-x_2)/(x_1-x_2)*(x-x_3)/(x_1-x_3)*(x-x_4)/(x_1-x_4)*(x-x_5)/(x_1-x_5)*(x-x_6)/(x_1-x_6);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">Lx_2 :=(x-x_1)/(x_2-x_1)*(x-x_3)/(x_2-x_3)*(x-x_4)/(x_2-x_4)*(x-x_5)/(x_2-x_5)*(x-x_6)/(x_2-x_6);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">Lx_3 :=(x-x_1)/(x_3-x_1)*(x-x_2)/(x_3-x_2)*(x-x_4)/(x_3-x_4)*(x-x_5)/(x_3-x_5)*(x-x_6)/(x_3-x_6);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">Lx_4 :=(x-x_1)/(x_4-x_1)*(x-x_2)/(x_4-x_2)*(x-x_3)/(x_4-x_3)*(x-x_5)/(x_4-x_5)*(x-x_6)/(x_4-x_6);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">Lx_5 :=(x-x_1)/(x_5-x_1)*(x-x_2)/(x_5-x_2)*(x-x_3)/(x_5-x_3)*(x-x_4)/(x_5-x_4)*(x-x_6)/(x_5-x_6);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">Lx_6 :=(x-x_1)/(x_6-x_1)*(x-x_2)/(x_6-x_2)*(x-x_3)/(x_6-x_3)*(x-x_4)/(x_6-x_4)*(x-x_5)/(x_6-x_5);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle3" style="_cstyle3"/></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">dLx_1 := diff(Lx_1,x):</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">dLx_2 := diff(Lx_2,x):</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">dLx_3 := diff(Lx_3,x):</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">dLx_4 := diff(Lx_4,x):</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">dLx_5 := diff(Lx_5,x):</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">dLx_6 := diff(Lx_6,x):</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle3" style="_cstyle3"/></Input></Group><Group><Input><Text-field layout="_pstyle3" prompt="&gt; " style="ParagraphStyle1"><Font style="_cstyle2">with(codegen,fortran):</Font><Font style="_cstyle3">   </Font></Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fortran(dLx_1);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fortran(dLx_2);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fortran(dLx_3);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fortran(dLx_4);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fortran(dLx_5);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fortran(dLx_6);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2"/></Input></Group></Section><Section collapsed="true"><Title><Text-field layout="_pstyle1" style="_cstyle1">Esquema de sexta ordem compacto:</Text-field></Title><Text-field layout="_pstyle3" style="_cstyle3"/><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">restart;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">P:=(x-x_2n)*(x-x_0)*(x-x_2p); diff(P,x); diff(diff(P,x),x); </Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">Px_1n  :=(x_1n-x_2n)*(x_1n-x_0)*(x_1n-x_2p);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">Px_1p  :=(x_1p-x_2n)*(x_1p-x_0)*(x_1p-x_2p);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle3" style="_cstyle3"/></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">dPx_1n := (x_1n-x_0)*(x_1n-x_2p)+(x_1n-x_2n)*(x_1n-x_2p)+(x_1n-x_2n)*(x_1n-x_0);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">dPx_0  := (x_0 -x_0)*(x_0 -x_2p)+(x_0 -x_2n)*(x_0 -x_2p)+(x_0 -x_2n)*(x_0 -x_0);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">dPx_1p := (x_1p-x_0)*(x_1p-x_2p)+(x_1p-x_2n)*(x_1p-x_2p)+(x_1p-x_2n)*(x_1p-x_0);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle3" style="_cstyle3"/></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">d2Px_1n := 6.0*x_1n-2.0*x_2p-2.0*x_0-2.0*x_2n;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">d2Px_0  := 6.0*x_0 -2.0*x_2p-2.0*x_0-2.0*x_2n;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">d2Px_1p := 6.0*x_1p-2.0*x_2p-2.0*x_0-2.0*x_2n;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle3" style="_cstyle3"/></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">DD:= 6.0+4.0*(x_1p-x_1n)*(dPx_1p/Px_1p-dPx_1n/Px_1n)-2.0*(x_1p-x_1n)**2*(dPx_1p/Px_1p*dPx_1n/Px_1n);</Text-field></Input></Group><Section><Title><Text-field layout="_pstyle4" style="_cstyle4">Coeficientes para a segunda derivada (LHS)</Text-field></Title><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">B_1P := (-(2/(x_1n-x_1p) + dPx_1n/Px_1n)*(x_1n-x_1p)^2)/DD;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">B_2P := (1-(x_1p-x_1n)*dPx_1n/Px_1n)/DD;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">B_1N := (-(2/(x_1p-x_1n) + dPx_1p/Px_1p)*(x_1p-x_1n)^2)/DD;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">B_2N := (1-(x_1n-x_1p)*dPx_1p/Px_1p)/DD;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">a_1p:=( 2*dPx_0*(B_1P*(2*x_0-x_1p-x_1n)+B_2N*(x_0-x_1p)*(3*x_0-2*x_1n-x_1p))+ 
        d2Px_0*(x_0-x_1p)*(x_0-x_1n)*(B_1P+(x_0-x_1p)*B_2P) 
        ) / ((x_1p-x_1n)*Px_1n );</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">a_1n:=( 2*dPx_0*(B_1N*(2*x_0-x_1p-x_1n)+B_2N*(x_0-x_1n)*(3*x_0-2*x_1p-x_1n))+ 
        d2Px_0*(x_0-x_1p)*(x_0-x_1n)*(B_1N+(x_0-x_1n)*B_2N) 
        ) / ((x_1p-x_1n)*Px_1n );</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">with(codegen,fortran):</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fortran(a_1p);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fortran(a_1n);</Text-field></Input></Group></Section><Section><Title><Text-field layout="_pstyle4" style="_cstyle4">Coeficientes para a segunda derivada (RHS)</Text-field></Title><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">A_1p := (-4.0*dPx_1p/Px_1p 
         -2.0*dPx_1n/Px_1n  
         +2.0*(x_1p-x_1n)*(dPx_1p/Px_1p*dPx_1n/Px_1n-d2Px_1p/Px_1p)
         +(x_1p-x_1n)^2*d2Px_1p/Px_1p*dPx_1n/Px_1n)/DD;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">A_1n := (-4.0*dPx_1n/Px_1n 
         -2.0*dPx_1p/Px_1p  
         +2.0*(x_1n-x_1p)*(dPx_1p/Px_1p*dPx_1n/Px_1n-d2Px_1n/Px_1n)
         +(x_1n-x_1p)^2*d2Px_1n/Px_1n*dPx_1p/Px_1p)/DD;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">A_2p := (4.0*dPx_1p/Px_1p*dPx_1n/Px_1n 
         -d2Px_1p/Px_1p 
         -2/(x_1p-x_1n)*(dPx_1p/Px_1p-dPx_1n/Px_1n)
         +(x_1p-x_1n)*(d2Px_1p/Px_1p*dPx_1n/Px_1n))/DD;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">A_2n := (4.0*dPx_1p/Px_1p*dPx_1n/Px_1n 
         -d2Px_1n/Px_1n 
         -2/(x_1n-x_1p)*(dPx_1n/Px_1n-dPx_1p/Px_1p)
         +(x_1n-x_1p)*(d2Px_1n/Px_1n*dPx_1p/Px_1p))/DD;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">b_1n := (2.0*dPx_0*(1.0+A_1n*(2.0*x_0-x_1p-x_1n)+A_2n*(x_0-x_1n)*(3.0*x_0-2.0*x_1p-x_1n))
+d2Px_0*(x_0-x_1p)*(1.0+A_1n*(x_0-x_1n)+A_2n*(x_0-x_1n)^2))/((x_1n-x_1p)*Px_1n);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">b_1p := (2.0*dPx_0*(1.0+A_1p*(2.0*x_0-x_1p-x_1n)+A_2p*(x_0-x_1p)*(3.0*x_0-2.0*x_1n-x_1p))
+d2Px_0*(x_0-x_1n)*(1.0+A_1p*(x_0-x_1p)+A_2p*(x_0-x_1p)^2))/((x_1p-x_1n)*Px_1p);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">with(codegen,fortran):</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fortran(b_1n);</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">fortran(b_1p);</Text-field></Input></Group></Section></Section><Section collapsed="true"><Title><Text-field layout="_pstyle1" style="_cstyle1"><Font encoding="ISO8859-1">Aproxima\347\343o de Diferen\347as Finitas para Ordem N:</Font></Text-field></Title><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">restart;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">with(linalg):</Text-field></Input><Output><Text-field layout="Warning" style="Warning">Warning, the protected names norm and trace have been redefined and unprotected</Text-field></Output></Group><Group><Input><Text-field layout="Normal" prompt="&gt; " style="Maple Input">st_lhs     := 7:</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">center     := 0:</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">derivative := 1:</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">ordem      := st_lhs-1: ordem;</Text-field></Input><Output><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output">NiMiIic=</Equation></Text-field></Output></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">adjust     := ordem mod 2:</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">A := Matrix(ordem+1):</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">for ii from 1 by 1 to ordem+1 do  
  for jj from 1 by 1 to ordem+1 do  
    A[ii,jj]:= (jj-round(ordem/2.0)+adjust-1+center)^(ii-1)/(ii-1)!; 
  end do;
end do;</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">Matrix(A);</Text-field></Input><Output><Text-field layout="Maple Output" style="2D Output"><Equation style="2D Output"></Equation></Text-field></Output></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">b := vector(ordem+1,0):</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">b[derivative+1] := 1: </Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">r := linsolve(A, b, 'r');</Text-field></Input></Group><Group><Input><Text-field layout="_pstyle2" prompt="&gt; " style="_cstyle2">df     := 0:
hh     := 0:
